                            <div class="card border-dark">
                                <div class="card-header">Criterios Ingresados</div>
                                    <div class="card-body">
                                        <table class="table table-hover" name="tablaCargas" id="tablaCargas" style="width: 100%;">
                                            <tbody>
                                                {{#each empalmeMallasCargadas}}
                                                <tr>
                                                    <td>{{nombre}}</td>
                                                    <td>
                                                        <a href="#" onclick="cargaInfoMallaEmpalme({{id_empalme_malla_proyecto}});"> 
                                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-eye" viewBox="0 0 16 16" >
